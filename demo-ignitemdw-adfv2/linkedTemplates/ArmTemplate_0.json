{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demo-ignitemdw-adfv2"
		},
		"demo60_sftp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'demo60_sftp'"
		},
		"demo_ada_edl_SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demo_ada_edl_SQLDB'"
		},
		"privatestor_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'privatestor'"
		},
		"demo_shir_to_SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demo_shir_to_SQLDB'"
		},
