{
	"name": "moviesminusonecsv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "demo30_ADLS_MI",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"foldername": {
				"type": "string",
				"defaultValue": "data"
			},
			"filename": {
				"type": "string",
				"defaultValue": "movies.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileSystem": {
					"value": "@dataset().foldername",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "movie",
				"type": "String"
			},
			{
				"name": "title",
				"type": "String"
			},
			{
				"name": "genres",
				"type": "String"
			},
			{
				"name": "year",
				"type": "String"
			},
			{
				"name": "Rotton Tomato",
				"type": "String"
			}
		]
	}
}