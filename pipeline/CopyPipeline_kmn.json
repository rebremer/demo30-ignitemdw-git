{
	"name": "CopyPipeline_kmn",
	"properties": {
		"activities": [
			{
				"name": "ForEach_kmn",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_kmn",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.schema}.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "datadefinedarchive//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_kmn",
									"type": "DatasetReference",
									"parameters": {
										"cw_schema": "@item().source.schema",
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_kmn",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"schema": "dbo",
							"table": "AWBuildVersion"
						},
						"destination": {
							"fileName": "dboAWBuildVersion.txt"
						}
					},
					{
						"source": {
							"schema": "dbo",
							"table": "DatabaseLog"
						},
						"destination": {
							"fileName": "dboDatabaseLog.txt"
						}
					},
					{
						"source": {
							"schema": "dbo",
							"table": "ErrorLog"
						},
						"destination": {
							"fileName": "dboErrorLog.txt"
						}
					},
					{
						"source": {
							"schema": "Person",
							"table": "Person"
						},
						"destination": {
							"fileName": "PersonPerson.txt"
						}
					},
					{
						"source": {
							"schema": "Person",
							"table": "PersonPhone"
						},
						"destination": {
							"fileName": "PersonPersonPhone.txt"
						}
					},
					{
						"source": {
							"schema": "Person",
							"table": "PhoneNumberType"
						},
						"destination": {
							"fileName": "PersonPhoneNumberType.txt"
						}
					},
					{
						"source": {
							"schema": "Person",
							"table": "StateProvince"
						},
						"destination": {
							"fileName": "PersonStateProvince.txt"
						}
					},
					{
						"source": {
							"schema": "Sales",
							"table": "Customer"
						},
						"destination": {
							"fileName": "SalesCustomer.txt"
						}
					}
				]
			}
		},
		"annotations": []
	}
}