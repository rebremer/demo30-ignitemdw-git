{
	"name": "demo30_onpremsource",
	"properties": {
		"description": "Source in sql server onprem instead of HTTP",
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "demo30_http_csv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "demo30_onpremsql_table",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}